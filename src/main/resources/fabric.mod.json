	"entrypoints": {
		"main": [
          "vandejr.learningtomod.LearningToMod"
		],
		"client":  [
			"vandejr.learningtomod.LearningToModClient"
		],
		"fabric-datagen": [
			"vandejr.learningtomod.LearningToModDataGenerator"
		]
	},
	"mixins": [
		"learningtomod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
